{
  "expo": {
    "scheme": "karma",
    "owner": "schmelman",
    "name": "karma",
    "slug": "karma",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#44337A"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "androidNavigationBar": {
      "backgroundColor": "#44337A",
      "barStyle": "light-content"
    },
    "androidStatusBar": {
      "backgroundColor": "#44337A",
      "barStyle": "light-content"
    }
  }
}
