{
  "cli": {
    "version": ">= 0.52.0"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "APP_VARIANT": "development"
      },
      "releaseChannel": "devel"
    },
    "developmentStore": {
      "distribution": "internal",
      "env": {
        "APP_VARIANT": "production"
      },
      "releaseChannel": "prod"
    },
    "production": {
      "env": {
        "APP_VARIANT": "production"
      },
      "releaseChannel": "prod"
    }
  },
  "submit": {
    "production": {}
  }
}
